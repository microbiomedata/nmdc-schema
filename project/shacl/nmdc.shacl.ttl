nmdc:MetatranscriptomeAnnotationActivity a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Example: https://github.com/microbiomedata/mg_annotation/releases/tag/0.1" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path nmdc:git_url ],
        [ sh:description "A human readable label for an entity" ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path nmdc:name ],
        [ sh:class nmdc:NamedThing ;
            sh:description "An output biosample to a processing step" ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 5 ;
            sh:path nmdc:has_output ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 9 ;
            sh:path nmdc:started_at_time ;
            sh:pattern "^([\\+-]?\\d{4}(?!\\d{2}\\b))((-?)((0[1-9]|1[0-2])(\\3([12]\\d|0[1-9]|3[01]))?|W([0-4]\\d|5[0-2])(-?[1-7])?|(00[1-9]|0[1-9]\\d|[12]\\d{2}|3([0-5]\\d|6[1-6])))([T\\s]((([01]\\d|2[0-3])((:?)[0-5]\\d)?|24\\:?00)([\\.,]\\d+(?!:))?)?(\\17[0-5]\\d([\\.,]\\d+)?)?([zZ]|([\\+-])([01]\\d|2[0-3]):?([0-5]\\d)?)?)?)?$" ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 10 ;
            sh:path nmdc:ended_at_time ;
            sh:pattern "^([\\+-]?\\d{4}(?!\\d{2}\\b))((-?)((0[1-9]|1[0-2])(\\3([12]\\d|0[1-9]|3[01]))?|W([0-4]\\d|5[0-2])(-?[1-7])?|(00[1-9]|0[1-9]\\d|[12]\\d{2}|3([0-5]\\d|6[1-6])))([T\\s]((([01]\\d|2[0-3])((:?)[0-5]\\d)?|24\\:?00)([\\.,]\\d+(?!:))?)?(\\17[0-5]\\d([\\.,]\\d+)?)?([zZ]|([\\+-])([01]\\d|2[0-3]):?([0-5]\\d)?)?)?)?$" ],
        [ sh:class nmdc:NamedThing ;
            sh:description "An input to a process." ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 4 ;
            sh:path nmdc:has_input ],
        [ sh:description "A unique identifier for a thing. Must be either a CURIE shorthand for a URI or a complete URI" ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path nmdc:id ],
        [ sh:description "identifiers for corresponding analysis project in GOLD" ;
            sh:order 1 ;
            sh:path nmdc:gold_analysis_project_identifiers ;
            sh:pattern "^GOLD:Ga[0-9]+$" ],
        [ sh:maxCount 1 ;
            sh:order 13 ;
            sh:path nmdc:used ],
        [ sh:description "An optional string that specifies the type object.  This is used to allow for searches for different kinds of objects." ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path nmdc:type ],
        [ sh:class nmdc:NamedThing ;
            sh:description "Links a resource to another resource that either logically or physically includes it." ;
            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path dcterms:isPartOf ],
        [ sh:class nmdc:WorkflowExecutionActivity ;
            sh:description "the agent/entity associated with the generation of the file" ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 12 ;
            sh:path nmdc:was_associated_with ],
        [ sh:description "Example: NERSC-Cori" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path nmdc:execution_resource ],
        [ sh:class nmdc:Activity ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 11 ;
            sh:path nmdc:was_informed_by ] ;
    sh:targetClass nmdc:MetatranscriptomeAnnotationActivity .

nmdc:MetatranscriptomeAssembly a sh:NodeShape ;
