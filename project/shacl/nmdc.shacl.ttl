OBI:0000011 a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Planned Process" ;
    sh:property [ sh:class nmdc:NamedThing ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path nmdc:has_inputs ],
        [ sh:description "A human readable label for an entity" ;
            sh:maxCount 1 ;
            sh:order 4 ;
            sh:path nmdc:name ],
        [ sh:class nmdc:Agent ;
            sh:maxCount 1 ;
            sh:nodeKind sh:BlankNode ;
            sh:order 2 ;
            sh:path nmdc:participating_agent ],
        [ sh:description "A list of alternative identifiers for the entity." ;
            sh:order 6 ;
            sh:path nmdc:alternative_identifiers ],
        [ sh:description "A unique identifier for a thing. Must be either a CURIE shorthand for a URI or a complete URI" ;
            sh:maxCount 1 ;
            sh:order 3 ;
            sh:path nmdc:id ],
        [ sh:description "a human-readable description of a thing" ;
            sh:maxCount 1 ;
            sh:order 5 ;
            sh:path dcterms:description ],
        [ sh:class nmdc:NamedThing ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path nmdc:has_outputs ] ;
    sh:targetClass OBI:0000011 .

