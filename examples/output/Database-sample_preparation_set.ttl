@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix generic: <https://example.org/generic/> .
@prefix nmdc: <https://w3id.org/nmdc/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

generic:Campaign_1 a nmdc:Campaign ;
    dcterms:description "Some description" ;
    nmdc:campaign_category "obtaining_solutes" ;
    nmdc:has_part generic:Filtering_1,
        generic:Sieving_1,
        generic:Suspending_1 ;
    nmdc:name "Some name" ;
    nmdc:protocol_link [ a nmdc:Protocol ;
            nmdc:name "Red Velvet Cupcake Protocol" ;
            nmdc:url "https://dx.doi.org/10.17504/protocols.io.bbh7ij9n" ] .

generic:Filtering_1 a "nmdc:Filtering"^^xsd:anyURI ;
    nmdc:has_input generic:suspension_1 ;
    nmdc:has_output generic:filtrate_1,
        generic:oversize_1 ;
    nmdc:paper_grade "grade_3" .

generic:Sieving_1 a "nmdc:Sieving"^^xsd:anyURI ;
    nmdc:has_input generic:Biosample_1 ;
    nmdc:has_output generic:coarse_1,
        generic:fines_1 ;
    nmdc:mesh_size_mm "3.0"^^xsd:float .

generic:Suspending_1 a "nmdc:Suspending"^^xsd:anyURI ;
    nmdc:has_input generic:fines_1 ;
    nmdc:has_output generic:suspension_1 ;
    nmdc:stir_rate_rpm "1000.0"^^xsd:float ;
    nmdc:stir_time_min "10.0"^^xsd:float .

[] a nmdc:Database ;
    nmdc:campaign_set generic:Campaign_1 ;
    nmdc:sample_preparation_set generic:Filtering_1,
        generic:Sieving_1,
        generic:Suspending_1 .

