@prefix nmdc: <https://w3id.org/nmdc/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

nmdc:mp-1 nmdc:designated_class "nmdc:MixingProcess"^^xsd:anyURI ;
    nmdc:duration [ a nmdc:QuantityValue ;
            nmdc:has_numeric_value 2e+00 ;
            nmdc:has_unit "hours" ] ;
    nmdc:has_input nmdc:biosample-1,
        nmdc:processed-sample-2 ;
    nmdc:has_output nmdc:4 ;
    nmdc:instrument_name "Orbital Shaker" .

